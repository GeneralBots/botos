{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "BotOS default capabilities",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "dialog:default",
    "opener:default",
    "notification:default",
    "http:default",
    "geolocation:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://**" },
        { "url": "http://**" }
      ]
    }
  ]
}
